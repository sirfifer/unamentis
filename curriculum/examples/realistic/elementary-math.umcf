  "glossary": {
    "terms": [
      {
        "id": "term-multiply",
        "term": "multiply",
        "pronunciation": "/ˈməltəˌplī/",
        "definition": "To add a number to itself a certain number of times.",
        "spokenDefinition": "To multiply means to add a number to itself a certain number of times. For example, three times four means adding three four times: three plus three plus three plus three.",
        "simpleDefinition": "Adding the same number over and over.",
        "synonyms": ["times"],
        "relatedTerms": ["product", "factor"]
      },
      {
        "id": "term-product",
        "term": "product",
        "pronunciation": "/ˈprädəkt/",
        "definition": "The answer when you multiply two numbers together.",
        "simpleDefinition": "The answer to a times problem.",
        "relatedTerms": ["multiply", "factor"]
      },
      {
        "id": "term-factor",
        "term": "factor",
        "pronunciation": "/ˈfaktər/",
        "definition": "A number that you multiply by another number.",
        "simpleDefinition": "The numbers you multiply together.",
        "relatedTerms": ["multiply", "product"]
      },
      {
        "id": "term-array",
        "term": "array",
        "pronunciation": "/əˈrā/",
        "definition": "Objects arranged in rows and columns that help us see multiplication.",
        "simpleDefinition": "Things lined up in rows and columns, like seats in a movie theater.",
        "relatedTerms": ["row", "column"]
      }
    ]
  },
